@![sql]
@@@@@@@@@@@@@@@@@@
select t1.$2, t2.$2
from tab("/tmp/t1.txt") as t1,
     tab("/tmp/t2.txt") as t2
where (t1.$1 == t2.$1 and t1.$1 < 50) and (t1.$1 == t1.$1 or t2.$2 == t2.$2)
@==================

@![result]
